        // CUMULATIVE: "Low+" means Low and above (L, M, H, VH)
        // =====================================================================

        "Baseline (Medium+)": {
            "category": "Intensity",
            "params": {
                "intensity": "M",
                "timescale": "All",
                "direct_indirect": "All",
                "spatial": "All",
                "comp_svc_rating": "All",
                "svc_act_intensity": "M"
            },
            "description": "The baseline preset with Medium+ pressure threshold. Reference point for all comparisons.",
            "use_case": "Reference point for comparing the effect of individual parameter changes."
        },

        "Low+": {
            "category": "Intensity",
            "params": {
                "intensity": "L",
                "timescale": "All",
                "direct_indirect": "All",
                "spatial": "All",
                "comp_svc_rating": "All",
                "svc_act_intensity": "M"
            },
            "description": "Includes Low+ intensity pressures. Shows more connections including weaker environmental pressures from source activities.",
            "use_case": "Compare to Baseline to see emerging/weak pressure pathways."
        },

        "High+": {
            "category": "Intensity",
